<%= form_for Location.new do |l| %>
  <%= l.label :name %>
  <%= l.text_field :name %>

  <%= l.label :latitude %>
  <%= l.text_field :latitude %>

  <%= l.label :longitude %>
  <%= l.text_field :longitude %>

  <%= l.submit "Search", link_to: 'locations/create', class: "btn btn-primary" %>
<% end %>
